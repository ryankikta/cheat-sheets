server {
        listen 80;
#        listen 443;
#        ssl on;#
#   ssl_certificate /etc/letsencrypt/live/printaura.com/fullchain.pem;
#   ssl_certificate_key /etc/letsencrypt/live/printaura.com/privkey.pem;
    #enables SSLv3/TLSv1, but not SSLv2 which is weak and should no longer be used.
#   ssl_protocols SSLv3 TLSv1;
    #Disables all weak ciphers
#   ssl_ciphers ALL:!aNULL:!ADH:!eNULL:!LOW:!EXP:RC4+RSA:+HIGH:+MEDIUM;        
      
#   server_name  printaura.com www.printaura.com ;
    server_name 104.198.162.202;
    root "/home/printaura/public_html";
    port_in_redirect off;
    server_tokens off;
    autoindex off;
    disable_symlinks off;
    index index.php;
    client_max_body_size 400m;
    client_body_buffer_size 2048k;
    large_client_header_buffers 2048 2m;
    access_log  /home/printaura/logs/access.log;
    error_log /home/printaura/logs/default.log ;
    error_page 502 /502.html;
    location = /502.html {
        root  /home/printaura/public_html;
    }
    if ($http_user_agent ~* (Baiduspider|webalta|nikto|wkito|pikto|scan|acunetix|morfeus|webcollage|youdao) ) {
        return 401;
    }
    if ($http_user_agent ~* (HTTrack|clshttp|archiver|loader|email|harvest|extract|grab|miner) ) {
        return 401;
    }
    # expires 86400s;
    add_header Pragma public;
    add_header Cache-Control "max-age=86400, public, must-revalidate, proxy-revalidate";
    #add_header X-Frame-Options "SAMEORIGIN";
    gzip  on;
    gzip_http_version 1.1;
    gzip_vary on;
    gzip_comp_level 9;
    gzip_proxied any;
    gzip_types text/plain  text/css application/json application/x-javascript text/xml application/xml application/xml+rss text/javascript application/javascript text/x-js;
    gzip_buffers 16 8k;
    gzip_disable "MSIE [1-6]\.(?!.*SV1)";
    set $cache_uri $request_uri;
    location /image-wizard/ {
        error_log /var/log/nginx/uploads-error.log;
    }
    # POST requests and urls with a query string should always go to PHP
    if ($request_method = POST) {
        set $cache_uri 'null cache';
    }
    if ($query_string != "") {
        set $cache_uri 'null cache';
    }
    # Don't cache uris containing the following segments
    if ($request_uri ~* "(/wp-admin/|/xmlrpc.php|/wp-(app|cron|login|register|mail).php|wp-.*.php|/feed/|index.php|wp-comments-popup.php|wp-links-opml.php|wp-locations.php|sitemap(_index)?.xml|[a-z0-9_-]+-sitemap([0-9]+)?.xml)") {
        set $cache_uri 'null cache';
    }
    # Don't use the cache for logged in users or recent commenters
    if ($http_cookie ~* "comment_author|wordpress_[a-f0-9]+|wp-postpass|wordpress_logged_in") {
        set $cache_uri 'null cache';
    }
    # Use cached or actual file if they exists, otherwise pass request to WordPress
    location / {
        #  rewrite ^/strimages/(.*) /testdownload.php?source=$1 last;
        try_files /wp-content/w3tc/pgcache/$cache_uri/_index.html $uri $uri/ /index.php?$args ;
        rewrite ^/strimages/(.*)/(.*)/(.*)/(.*)/(.*).jpg$ /download.php?source=$1&f=$2&u=$3&ftype=$4 last;
        #      rewrite ^/(.*)/feed$    /feed last;
        #                 rewrite ^/api.php   /api/ last;
        #              rewrite ^/strimages/(.*)$ /testdownload.php?source=$1 last;
    }
    location /NewEtsyBusiness {
        rewrite ^/NewEtsyBusiness    /etsy-app#_a_6n permanent;
    }
    location = /favicon.ico { log_not_found off; access_log off; }
    location = /robots.txt  {  access_log off; }
    location ~* /(?:uploads|files|js\/uploadify|js)/.*\.php$ {
        deny all;
    }
    location ~ [^/]\.php(/|$)  {
            try_files $uri /index.php;
    #   try_files $uri =404;
        fastcgi_split_path_info ^(.+\.php)(/.+)$;
        if (!-f $document_root$fastcgi_script_name) {
            return 502;
        }
        include fastcgi_params;
        fastcgi_pass unix:/home/printaura/.phpbrew/php/php-5.4.41/var/run/printaura_fpm.sock;
        fastcgi_param PATH_TRANSLATED $document_root$fastcgi_script_name;
        #      fastcgi_intercept_errors on;
        # fastcgi_ignore_client_abort off;
        fastcgi_connect_timeout 1000000;
        fastcgi_send_timeout 1000000;
        fastcgi_read_timeout 1000000;
        fastcgi_buffer_size 512k;
        fastcgi_buffers 4 512k;
        fastcgi_busy_buffers_size 512k;
        fastcgi_temp_file_write_size 512k;
    }
    # Cache static files for as long as possible
    location ~* .(ogg|ogv|svg|svgz|eot|otf|woff|mp4|ttf|css|rss|atom|js|jpg|jpeg|gif|png|ico|zip|tgz|gz|rar|bz2|doc|xls|exe|ppt|tar|mid|midi|wav|bmp|rtf)$ {
           expires 2592000s; log_not_found off; access_log off; add_header Pragma public;add_header Cache-Control "max-age=2592000, public";
    }
    location ~ ^/(status_pa|ping)$ {
        access_log off;
        #     deny all;
        include fastcgi_params;
        # This is important
        fastcgi_index index.php;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
        #     include fastcgi_params;
        fastcgi_pass unix:/home/printaura/.phpbrew/php/php-5.4.41/var/run/printaura_fpm.sock;
    }
    location ~ /\. {
        deny all;
    }
    #      location ~* /(?:uploads|files|js\/uploadify)/.*\.php$ {
    #       deny all;
    #      }
    location ~* /\.(ht|git|svn) {
        deny  all;
    }
}
server {
    listen 443;
    ssl on;
    ssl_certificate /etc/ssl/certs/printaura_com.pem;
    ssl_certificate_key /etc/ssl/private/pa-2018-pkey.key;
    #ssl_certificate /etc/letsencrypt/live/printaura.com/fullchain.pem;
    #ssl_certificate_key /etc/letsencrypt/live/printaura.com/privkey.pem;
    #enables SSLv3/TLSv1, but not SSLv2 which is weak and should no longer be used.
    ssl_protocols SSLv3 TLSv1;
    #Disables all weak ciphers
    ssl_ciphers ALL:!aNULL:!ADH:!eNULL:!LOW:!EXP:RC4+RSA:+HIGH:+MEDIUM;
    server_name http://104.198.162.202;
    root "/home/printaura/public_html";
    port_in_redirect off;
    server_tokens off;
    autoindex off;
    index index.php;
    client_max_body_size 400m;
    client_body_buffer_size 1024k;
    large_client_header_buffers 16 512k;
    access_log  /home/printaura/logs/access.log;
    error_log /home/printaura/logs/default.log ;
    error_page 502 /502.php;
    location = /502.php {
        root  /home/printaura/public_html;
    }
    if ($http_user_agent ~* (Baiduspider|webalta|nikto|wkito|pikto|scan|acunetix|morfeus|webcollage|youdao) ) {
        return 401;
    }
    if ($http_user_agent ~* (HTTrack|clshttp|archiver|loader|email|harvest|extract|grab|miner) ) {
        return 401;
    }
     expires 86400s;
    add_header Pragma public;
    add_header Cache-Control "max-age=86400, public, must-revalidate, proxy-revalidate";
    #add_header X-Frame-Options "SAMEORIGIN";
    gzip  on;
